{
  "version": 2,
  "name": "sharing-caring",
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "api/upload.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/download.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/api/upload",
      "dest": "/api/upload.js",
      "methods": ["POST", "OPTIONS"]
    },
    {
      "src": "/api/download/(.*)",
      "dest": "/api/download.js",
      "methods": ["GET", "OPTIONS"]
    }
  ]
}
